{
  "name": "@1inch/solana-fusion-sdk",
  "version": "0.1.0",
  "description": "SDK for fusion on Solana",
  "author": "@1inch",
  "license": "MIT",
  "private": false,
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./utils": {
      "types": "./dist/types/src/utils.d.ts",
      "import": "./dist/esm/utils.js",
      "require": "./dist/cjs/utils.js",
      "node": "./dist/cjs/utils.js",
      "default": "./dist/cjs/utils.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:1inch/solana-fusion-sdk.git"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.types.json",
    "sync:idl": "tsx scripts/sync-idl.ts",
    "sync:programs": "tsx scripts/sync-programs.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "eslint  \"{src,apps,libs,test,tests}/**/*.ts\" --fix",
    "lint:types": "tsc --noEmit",
    "test": "TZ=UTC jest",
    "test:integration": "TZ=UTC jest -c jest.integration.config.js",
    "test:watch": "jest --watch",
    "changelog:generate": "changelog generate -a",
    "qa:fix": "npm run build && npm run format && npm run lint && npm run lint:types && npm run test"
  },
  "devDependencies": {
    "@1inch/eslint-config": "latest",
    "@1inch/tsconfig": "^1.0.0",
    "@solana/keys": "2.0.0",
    "@solana/spl-token": "0.4.12",
    "@swc/core": "1.10.18",
    "@swc/jest": "0.2.37",
    "@types/bn.js": "5.1.6",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.4",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "~8.24.1",
    "@typescript-eslint/parser": "~8.24.1",
    "camelcase": "8.0.0",
    "eslint": "9.20.1",
    "eslint-config-prettier": "~10.0.1",
    "eslint-config-standard": "17",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "generate-changelog": "^1.8.0",
    "jest": "29.7.0",
    "prettier": "^3.5.1",
    "solana-bankrun": "0.4.0",
    "tsx": "4.19.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "node": "20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "bigint-buffer",
      "bufferutil",
      "esbuild",
      "utf-8-validate"
    ]
  },
  "dependencies": {
    "@1inch/byte-utils": "3.1.0",
    "@coral-xyz/anchor": "0.30.1",
    "@solana/web3.js": "1",
    "borsh": "2.0.0",
    "bs58": "6.0.0",
    "tslib": "2.8.1",
    "type-fest": "4.35.0",
    "ws": "^8.18.1"
  }
}
